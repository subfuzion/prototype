{
  "name": "prototype",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "check": "prettier . --check && eslint .",
    "fix": "prettier . --write && eslint . --fix",
    "test": "scripts/jest",
    "test:v": "npm test -- --silent=0",
    "test:w": "npm test -- --watch",
    "test:vw": "npm test -- --silent=0 --watch",
    "astro:build": "astro build",
    "astro:preview": "astro preview",
    "clean": "rimraf dist",
    "preclean:all": "npm run clean",
    "clean:all": "rimraf node_modules",
    "npm:outdated": "npm --loglevel=notice outdated",
    "npm:list": "npm --loglevel=notice run-script"
  },
  "dependencies": {
    "astro": "4.6.1"
  },
  "devDependencies": {
    "@eslint/js": "9.0.0",
    "@jest/globals": "29.7.0",
    "@stylistic/eslint-plugin": "1.7.0",
    "@types/jest": "29.5.12",
    "eslint": "9.0.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "rimraf": "5.0.5"
  }
}
